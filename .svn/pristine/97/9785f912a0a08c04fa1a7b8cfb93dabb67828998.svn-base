	    api.call({
            type: 'tel_prompt',
            number: '400-005-0571'
        });
	}
	
	function confirm(){
	    api.confirm({
	    title: '客服电话',
	    msg: '拨打客服电话400-005-0571',
	    buttons: ['拨打', '取消']
